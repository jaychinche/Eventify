                        <a href="https://eventify-1-ixhs.onrender.com/about" class="nav-item nav-link">About Us</a>
                        <!-- <a href="Ot.html" class="nav-item nav-link">Organize Talks</a> -->
                        <a href="http://eventify-1-ixhs.onrender.com/dashboard" class="nav-item nav-link">Contact</a>

                        <% if (user.userType==='admin' ) { %>
                            <a href="http://eventify-1-ixhs.onrender.com/events" class="nav-item nav-link">Add Event</a>
                            <% } %>

                                <% if(!token){%>
                                    <a href="http://eventify-1-ixhs.onrender.com/users/login" class="nav-item nav-link">Login</a>
                                    <%}%>
                                    
                                    <% if(token){%>
                                        <a href="http://eventify-1-ixhs.onrender.com/users/logout" class="nav-item nav-link">Log
                                            out</a>
                                            <%}%>
                    </div>
                    <% if (user.userType==='admin' ) { %>

<body data-bs-spy="scroll" data-bs-target=".navbar" data-bs-offset="51">
    <%if(success && success.length){%>
        <div
          class="alert alert-success alert-dismissible fade show col-7 offset-4 mt-1"
          role="alert"
        >
          <%=success%>
          <button
            type="button"
            class="btn-close"
            data-bs-dismiss="alert"
            aria-label="Close"
          ></button>
        </div>
        <%} %> <%if(error && error.length){%>
        <div
          class="alert alert-danger alert-dismissible fade show col-6 offset-4 mt-1"
          role="alert"
        >
          <%=error%>
          <button
            type="button"
            class="btn-close"
            data-bs-dismiss="alert"
            aria-label="Close"
          ></button>
        </div>
        <%} %>

                    </ul>
                    <div class="tab-content text-start">
                        <div id="tab-1" class="tab-pane fade show p-0 active">
                            <div class="row g-4">
                                <% events.forEach(event=> { %>
                                    <div class="col-lg-4">
                                        <div class="bg-light rounded">
                                            <img src="img/hack.jpg" class="img-fluid rounded-top"
                                                alt="<%= event.title %> Image">
                                            <div class="border-bottom p-4 mb-4">
                                                <h4 class="text-primary-gradient mb-1">
                                                    <%= event.title %>
                                                </h4>
                                                <span>
                                                    <%= event.content%>
                                                </span>

                                            </div>
                                            <div class="p-4 pt-0">
                                                <% if (user.userType!=='admin' ) { %>

                                                <a href="http://eventify-1-ixhs.onrender.com/register/<%= event._id %>"
                                                    class="btn btn-primary-gradient rounded-pill py-2 px-4 mt-4">Register</a>
                                                    <% } %>
                                              
                                                    <a href="/register/<%= event._id %>/view"
                                                        class="btn btn-primary-gradient rounded-pill py-2 px-4 mt-4">View
                                                        Deatils</a>
                                            </div>
                                        </div>
                                    </div>
                                    <% }); %>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
